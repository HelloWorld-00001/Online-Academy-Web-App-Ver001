        }
        .star-widget label{
            font-size: 40px;
            color: #fd4;
            padding: 10px;
            float: right;
            transition: all 0.2s ease;
        }
        input:not(:checked) ~ label:hover,
        input:not(:checked) ~ label:hover ~ label{
            color: #fd4;
            font-weight: 700;
        }
        input:checked ~ label{
            color: #fd4;
            font-weight: 700;
        }
        input#rate-5:checked ~ label{
            color: #fe7;
            text-shadow: 0 0 20px #952;
        }
        #rate-1:checked ~ .des-input header:before{
            content: "I just hate it üò†";
        }
        #rate-2:checked ~ .des-input header:before{
            content: "I don't like it üòí";
        }
        #rate-3:checked ~ .des-input header:before{
            content: "It is awesome üòÑ";
        }
        #rate-4:checked ~ .des-input header:before{
            content: "I just like it üòé";
        }
        #rate-5:checked ~ .des-input header:before{
            content: "I just love it üòç";
        }
        .container-review form{
            display: none;
        }
        input:checked ~ form{
            display: block;
        }
        form header{
            width: 100%;
            font-size: 25px;
            color: #fe7;
            font-weight: 500;
            margin: 5px 0 20px 0;
            /*text-align: center;*/
            transition: all 0.2s ease;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        form .textarea{
